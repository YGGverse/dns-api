{
  "net":
  {
    "dig":
    {
      "response":
      {
        "headers":
        {
          "Access-Control-Allow-Origin":"*"
        }
      },
      "request":
      {
        "name":{
          "regex":
          [
            "/\\.ygg$/i",
            "/\\.btn$/i",
            "/\\.anon$/i",
            "/\\.conf$/i",
            "/\\.index$/i",
            "/\\.index$/i",
            "/\\.merch$/i",
            "/\\.mirror$/i",
            "/\\.mob$/i",
            "/\\.screen$/i",
            "/\\.srv$/i"
          ]
        },
        "record":{
          "regex":
          [
            "/^A$/i",
            "/^AAAA$/i",
            "/^SRV$/i"
          ]
        },
        "records":{
          "regex":
          [
            "/^A$/i",
            "/^AAAA$/i",
            "/^SRV$/i"
          ]
        }
      }
    },
    "socket":
    {
      "response":
      {
        "headers":
        {
          "Access-Control-Allow-Origin":"*"
        }
      },
      "request":
      {
        "port":{
          "regex":
          [
            "/^[\\d]{2,6}$/"
          ]
        },
        "host":{
          "regex":
          [
            "/^0{0,1}[2-3][a-f0-9]{0,2}:/",
            "/\\.ygg$/i",
            "/\\.btn$/i",
            "/\\.anon$/i",
            "/\\.conf$/i",
            "/\\.index$/i",
            "/\\.index$/i",
            "/\\.merch$/i",
            "/\\.mirror$/i",
            "/\\.mob$/i",
            "/\\.screen$/i",
            "/\\.srv$/i"
          ]
        }
      }
    }
  }
}